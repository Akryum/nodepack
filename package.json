{
  "private": true,
  "workspaces": [
    "packages/@nodepack/*",
    "packages/test/*"
  ],
  "scripts": {
    "clean": "rimraf packages/test/*",
    "sync": "node scripts/syncDeps.js",
    "release": "yarn clean && yarn --pure-lockfile && node scripts/release.js",
    "test:types": "tsc",
    "test:lint": "eslint packages/@nodepack",
    "test:lerna-all": "lerna run test",
    "changelog": "node scripts/genChangelog.js run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@nodepack/conventional-changelog": "^0.1.0",
    "@types/ejs": "^2.6.0",
    "@types/events": "^1.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/globby": "^8.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/lru-cache": "^4.1.1",
    "@types/node": "^10.12.5",
    "@types/ora": "^1.3.4",
    "@types/semver": "^6.0.1",
    "@types/systeminformation": "^3.23.1",
    "@types/webpack-env": "^1.13.6",
    "@vuepress/plugin-pwa": "^1.0.2",
    "chalk": "^2.4.1",
    "conventional-changelog": "^3.0.6",
    "eslint": "^5.9.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-vue-libs": "^3.0.0",
    "execa": "^2.0.0",
    "globby": "^8.0.1",
    "inquirer": "^6.2.0",
    "lerna": "^3.4.3",
    "minimist": "^1.2.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "semver": "^6.3.0",
    "typescript": "^3.1.6",
    "vuepress": "^1.0.2"
  }
}
